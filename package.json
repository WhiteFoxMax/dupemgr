{
  "name": "dupemgr",
  "version": "1.0.0",
  "description": "Windows Electron app for duplicate detection and file organization",
  "main": "dist/main/main.js",
  "homepage": "./",
  "scripts": {
    "start": "electron .",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run build-main -- --watch\" \"vite dev src/renderer\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "npm run build-main && npm run build-renderer",
    "build-main": "tsc -p src/main/tsconfig.json",
    "build-renderer": "vite build src/renderer",
    "preview-renderer": "vite preview src/renderer",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.{ts,tsx}",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "duplicate",
    "file",
    "organizer",
    "electron",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4",
    "eslint": "^8.56.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "electron-store": "^8.1.0",
    "exif-parser": "^0.1.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1"
  }
}
